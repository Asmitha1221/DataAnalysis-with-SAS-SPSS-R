#Pract-7
data("PlantGrowth")
head(PlantGrowth)
data <- data.frame(score, method)
anova_mode <- aov(score ~ method, data = data)
summary(anova_mode)

#Pract-8
data(ToothGrowth)
# View first few rows
head(ToothGrowth)
